<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="NavigationDrawerText">
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">16dp</item>
        <item name="android:includeFontPadding">false</item>

    </style>

    <style name="RoundedCorners">
        <item name="cornerSize">10dp</item>
    </style>

    <style name="CustomTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="boxCollapsedPaddingTop">0dp</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="boxStrokeWidthFocused">0dp</item>
        <item name="hintTextAppearance">@style/CustomHintText</item>
        <item name="hintEnabled">false</item>
    </style>

    <style name="CustomHintText" parent="TextAppearance.Design.Hint">
        <item name="android:textSize">0sp</item>
    </style>

    <style name="RatingBarColor">
        <item name="colorControlActivated">@color/colorRatingBarFilled</item>
        <item name="colorControlNormal">@color/colorRatingBarUnfilled</item>
    </style>



</resources>
